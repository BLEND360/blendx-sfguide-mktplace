# Snowflake Native App Deployment Configuration
# Copy this file to .env and fill in your specific values

# ============================================
# Snowflake Connection
# ============================================
# Name of your Snowflake CLI connection (configured with 'snow connection add')
SNOW_CONNECTION=mkt_blendx_demo

# ============================================
# Docker Registry
# ============================================
# Your Snowflake Docker registry URL
# Format: <orgname>-<account_name>.registry.snowflakecomputing.com/<database>/<schema>/<repo_name>
SNOWFLAKE_REGISTRY=wb19670-c2gpartners.registry.snowflakecomputing.com/spcs_app/napp/img_repo

# ============================================
# Docker Images
# ============================================
# Names for your Docker images (without registry prefix)
BACKEND_IMAGE=eap_backend
FRONTEND_IMAGE=eap_frontend
ROUTER_IMAGE=eap_router

# ============================================
# Snowflake Resources
# ============================================
# Role used to manage the application package
APP_PACKAGE_ROLE=naspcs_role

# Role used to install and use the application
APP_CONSUMER_ROLE=nac_test

# Database and schema for application package development
APP_DATABASE=spcs_app_test
APP_SCHEMA=napp

# Stage name for application files
APP_STAGE=app_stage

# ============================================
# Application Package & Instance
# ============================================
# Name of the application package
APP_PACKAGE_NAME=spcs_app_pkg_test

# Name of the application instance
APP_INSTANCE_NAME=spcs_app_instance_test

# Version label for the application
APP_VERSION=v1

# ============================================
# Compute Resources
# ============================================
# Compute pool name for running the application
COMPUTE_POOL=pool_nac

# Warehouse name for the application
WAREHOUSE=APP_WH

# ============================================
# Deployment Options
# ============================================
# Set to 'true' to skip Docker build (use existing images)
SKIP_BUILD=false

# Set to 'true' to skip Docker push (useful for local testing)
SKIP_PUSH=false

# Wait time in seconds after starting service
SERVICE_START_WAIT=30
